@using SpotifyAPI.Web.Models
@using Zebra.Models

@model MusicModels

@{
    ViewBag.Title = "MusicDetails";
}

<h2>Music</h2>

<div>
    <h4>Détails</h4>
    <hr />
    <p>
        Titre : 
        @Model.Title
    </p>

    <p>
        Artists : 
        @foreach (var art in Model.ID_User)
        {
            @art.Name
        }
    </p>

    <p>
        Genre : 
        @Model.Genre
    </p>

    <p>
        Prix : 
        @Model.prix
    </p>

    <p>
        Populatité : 
        @Model.Note
    </p>
</div>

