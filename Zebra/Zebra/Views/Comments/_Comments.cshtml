@model IEnumerable<Zebra.Models.CommentModels>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Class</th>
            <th>Country</th>
        </tr>
    </thead>
    @foreach (var comment in Model)
    {
        <tr>
            <td>@comment.ID</td>
            <td>@comment.ID_Music</td>
            <td>@comment.ID_User</td>
            <td>@comment.Content</td>
        </tr>
    }
</table>
